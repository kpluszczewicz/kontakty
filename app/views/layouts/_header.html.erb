<header>
  <div id="header">
    <h1>Konktakty</h1>
  </div> 

  <div id="profile">
    <% if signed_in? -%>
      <%= link_to "Profile", current_user %>
      <%= link_to "Settings", edit_user_path(current_user) %>
      <%= link_to "Sign out", signout_path, :method => :delete %>
    <% else -%>
      <%= link_to "Zaloguj się przez zewnętrzny serwis"  %> | 
      <%= link_to "Zaloguj się", signin_path  %> 
    <% end -%>
  </div>

    <h3 id="page_desc">Serwis do zbierania konktaktów</h3>
</header>

